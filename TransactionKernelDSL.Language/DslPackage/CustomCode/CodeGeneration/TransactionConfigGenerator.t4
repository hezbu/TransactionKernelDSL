<#+
    private string ConnectionStringGenerator()
    {
        string result = "";
        if(String.IsNullOrEmpty(this.TransactionModel.DatabaseServerInstance) == false)
        {
            result += "<connectionStrings>\r\n\t<add name=\"MainConnection\" connectionString=\"";
            result += "data source="+this.TransactionModel.DatabaseServerInstance+";integrated security=false;";
            if(String.IsNullOrEmpty(this.TransactionModel.DatabaseInitialCatalog) == false) result+= "initial catalog="+this.TransactionModel.DatabaseInitialCatalog+";";
            else  result+= "initial catalog={database name};";

            if(String.IsNullOrEmpty(this.TransactionModel.DatabaseUser) == false) result+= " User ID="+this.TransactionModel.DatabaseUser+";";
            else result+= " User ID={username};";
            
            
            if(String.IsNullOrEmpty(this.TransactionModel.DatabasePassword) == false) result+= " pwd="+this.TransactionModel.DatabasePassword+";";
            else result+= " pwd={password};";

            result += "\"/>\r\n</connectionStrings>";
        }

        return result; 
    }
 #>
<#+
    private string InstanceGenerator(TransactionModel m)
    {
        string result= "<add key=\"InstanceId\" value=\""+m.InstanceId.ToString()+"\" />\r\n";
        if (String.IsNullOrEmpty(m.SatelliteInstances) == false)
            {
                foreach (string s in m.SatelliteInstances.Split(new char[] { '|' }))
                {
                    int instanceTest = 0;
                    if (Int32.TryParse(s, out instanceTest) == true)
                    {
                    result+= "<!--<add key=\"InstanceId\" value=\""+instanceTest.ToString()+"\" />--> <!-- Satellite Instance, activate it on a different app.config file-->\r\n";;
                    }
                }
                
            }
        return result;
    }
 #>
<#+
    private bool ValidateModel(TransactionModel m)
    {
        return (!String.IsNullOrEmpty(m.Name)) && (!String.IsNullOrEmpty(m.Namespace)) && (m.InstanceId > 0);
    }
 #>
<#+
    private string ParserConfigGenerator()
    {
        string result="";

        var inputList = from layer in this.TransactionModel.TransactionLayers
                let engines = layer.InputTransactionEngines
                from engine in engines
                where ((engine.Parser != ParserType.NotTyped) && (engine.Parser != ParserType.PropietaryParser))
                select engine; 
   
        foreach (InputTransactionEngine e in inputList)
        {
            string sectionName = (String.IsNullOrEmpty(e.ParserSectionName)?e.Parser.ToString():e.ParserSectionName);
            if(result.Contains("name=\""+sectionName+"\"") == false)
            {
                result += "<section name=\""+sectionName+"\"   type=\"TransactionKernelDSL.Framework.V1.AbstractTransactionParserSection, TransactionKernelDSL.Framework, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null\" allowDefinition=\"Everywhere\" allowExeDefinition=\"MachineToApplication\" restartOnExternalChanges=\"true\"/>\r\n\t";        
            }
        }        

        var outputList = from layer in this.TransactionModel.TransactionLayers
                let engines = layer.OutputTransactionEngines
                from engine in engines
                where ((engine.Parser != ParserType.NotTyped) && (engine.Parser != ParserType.PropietaryParser))
                select engine; 

        foreach (OutputTransactionEngine e in outputList)
        {
            string sectionName = (String.IsNullOrEmpty(e.ParserSectionName)?e.Parser.ToString():e.ParserSectionName);
            if(result.Contains("name=\""+sectionName+"\"") == false)
            {
                result += "<section name=\""+sectionName+"\"   type=\"TransactionKernelDSL.Framework.V1.AbstractTransactionParserSection, TransactionKernelDSL.Framework, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null\" allowDefinition=\"Everywhere\" allowExeDefinition=\"MachineToApplication\" restartOnExternalChanges=\"true\"/>\r\n\t";        
            }
        }        

        return result;
    }
 #>
