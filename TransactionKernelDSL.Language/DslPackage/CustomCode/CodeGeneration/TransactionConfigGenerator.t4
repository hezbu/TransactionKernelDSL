<# 
    if(ValidateModel(this.TransactionModel))
    {  
#>
<#=            string.Format(delimiter,"App") #><?xml version="1.0" encoding="utf-8" ?>
<configuration>	
    <configSections>
	    <#=            ParserConfigGenerator() #>
	</configSections>
	<appSettings>
        <#= InstanceGenerator(this.TransactionModel) #>
		       <#
        if(this.TransactionModel.GetValueImplementationType == PersistableSourceType.SQLServerDatabase)
        {
        #>
        <add key="GetValueStoredProcedure" value="Sp_GetValue"/>
<#
        }
        if(this.TransactionModel.SequenceFactoryImplementationType == PersistableSourceType.SQLServerDatabase)
        {
        #>
        <add key="SequenceFactoryStoredProcedure" value="Sp_SequenceFactory"/>
<#
        }

        foreach(TransactionLayer l in this.TransactionModel.TransactionLayers)
        {
            foreach(TransactionSQLServerDataSource ds in l.TransactionSQLServerDataSources)
            {
                #><add key="<#=                    ds.Name.Replace(" ","")#>StoredProcedure" value="Sp_<#=                    ds.Name.Replace(" ","")#>"/><#
            }
            
            var dssOeList = from dss in l.TransactionDataSourceSupports
                    where (dss.SupportType == DataSourceSupportType.SQLServerSupport
                        && dss.OutputTransactionEngines.Count > 0
                        )
                    let oes = dss.OutputTransactionEngines
                    from oe in oes
                    select oe;

            foreach(OutputTransactionEngine oe in dssOeList)
            {
                switch(oe.Type)
                {
                    case OutputEngineType.TcpFunneledOutputEngine:
  
                        #><add key="<#=                            "TL"+oe.TransactionLayer.Level+"_"+oe.TransactionLayer.Name.Replace(" ","")+ "_"+oe.Name.Replace(" ","")+"_Engine_"   #>PushRequirementStoredProcedure" value="Sp_<#=                            oe.TransactionLayer.TransactionModel.Name.Replace(" ","") #>FunneledEnginePushRequirementStoredProc"/><#=                            "\r\n" #><#
                        #><add key="<#=                            "TL"+oe.TransactionLayer.Level+"_"+oe.TransactionLayer.Name.Replace(" ","")+ "_"+oe.Name.Replace(" ","")+"_Engine_"   #>PullResponseStoredProcedure" value="Sp_<#=                            oe.TransactionLayer.TransactionModel.Name.Replace(" ","") #>FunneledEnginePullResponseStoredProc"/><#=                            "\r\n" #><#
                        #><add key="<#=                            "TL"+oe.TransactionLayer.Level+"_"+oe.TransactionLayer.Name.Replace(" ","")+ "_"+oe.Name.Replace(" ","")+"_Engine_"   #>UponForwardingProcessStoredProcedure" value="Sp_<#=                            oe.TransactionLayer.TransactionModel.Name.Replace(" ","") #>FunneledEngineUponForwardingProcessStoredProc"/><#=                            "\r\n" #><#
                        #><add key="<#=                            "TL"+oe.TransactionLayer.Level+"_"+oe.TransactionLayer.Name.Replace(" ","")+ "_"+oe.Name.Replace(" ","")+"_Engine_"   #>PushResponseStoredProcedure" value="Sp_<#=                            oe.TransactionLayer.TransactionModel.Name.Replace(" ","") #>FunneledEnginePushResponseStoredProc"/><#=                            "\r\n" #><#
                        #><add key="<#=                            "TL"+oe.TransactionLayer.Level+"_"+oe.TransactionLayer.Name.Replace(" ","")+ "_"+oe.Name.Replace(" ","")+"_Engine_"   #>PullAvailableRequirementsStoredProcedure" value="Sp_<#=                            oe.TransactionLayer.TransactionModel.Name.Replace(" ","") #>FunneledEnginePullAvailableRequirementsStoredProc"/><#=                            "\r\n" #><#
                        break;
